/!* keyword: scs_divider_double_semicolon */
scs_divider_double_semicolon => nrel_main_ru_idtf: 
	[sc.s-разделитель двойная точка с запятой] ;;

scs_divider_double_semicolon <= nrel_inclusion:
	scs_divider;;
scs_divider_double_semicolon <= nrel_representation_scs_editor:
	[;;];;
scs_divider_double_semicolon <= nrel_representation_standart_editor:
	[;;];;
scs_divider_double_semicolon <= nrel_explanation:
